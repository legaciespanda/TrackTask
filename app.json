{
  "expo": {
    "name": "Track-Task",
    "slug": "Track-Task",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "description": "A Task Management Mobile Application",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nubiaville.tracktask"
    },
    "web": {
      "favicon": "./assets/splash.png"
    }
  }
}
